{
    "version": 2,
    "name": "personal-website-admin-api",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "ADMIN_PASSWORD": "@admin_password",
        "FRONTEND_URL": "https://www.colemanlai.com"
    },
    "domains": [
        "api.colemanlai.com"
    ]
}